#include <bits/stdc++.h>
using namespace std;

#define ll long long

int main()
{
    int n;
    cin >> n;
    int arr[n];
    for (int i = 0; i < n; i++)
    {
        cin >> arr[i];
    }
    int x;
    cin >> x;
    int first = -1, last = -1;
    for (int i = 0; i < n; i++)
    {

        if (arr[i] == x)
        {

            if (first == -1)
                first = i;
            last = i;
        }
    }

    cout << "[" << first << ", " << last << "]";

    return 0;
}
